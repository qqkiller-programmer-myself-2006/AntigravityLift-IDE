{
    "$schema": "https://schema.tauri.app/config/2",
    "identifier": "default",
    "description": "Capability for the main window",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "shell:allow-open",
        "fs:default",
        "fs:allow-read-dir",
        "fs:allow-read-file",
        "fs:allow-write-file",
        "fs:allow-exists",
        "dialog:default",
        "dialog:allow-open",
        "dialog:allow-save"
    ]
}